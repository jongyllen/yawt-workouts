                    "id": "circuit",
                    "name": "Main Circuit",
                    "type": "circuit",
                    "rounds": 3,
                    "steps": [
                        {
                            "id": "c1",
                            "type": "exercise_inline",
                            "name": "Sit-to-stand squats",
                            "description": "Controlled quad work",
                            "instructions": "Stand in front of a chair. Sit down slowly until your glutes touch the seat, then immediately stand back up.",
                            "cues": [
                                "Controlled descent",
                                "Don't 'plop' down"
                            ],
                            "reps": 15
                        },
                        {
                            "id": "c2",
                            "type": "exercise_inline",
                            "name": "Reverse lunges",
                            "description": "Unilateral leg strength",
                            "instructions": "Step backward and lower your rear knee toward the floor. Keep your front shin vertical. Step back to start and switch legs.",
                            "cues": [
                                "Stay upright",
                                "90-90 knee angles"
                            ],
                            "reps": 20
                        },
                        {
                            "id": "c3",
                            "type": "exercise_inline",
                            "name": "Calf raises",
                            "description": "Lower leg endurance",
                            "instructions": "Stand on the edge of a step. Rise onto your toes, pause, and lower your heels slightly below the step level.",
                            "cues": [
                                "Full vertical reach",
                                "Pause at top"
                            ],
                            "reps": 20
                        }
                    ]
                },
                {
                    "id": "cooldown",
                    "name": "Cooldown",
                    "type": "cooldown",
                    "rounds": 1,
                    "steps": [
                        {
                            "id": "cd1",
                            "type": "hold",
                            "name": "Quad Stretch",
                            "description": "Leg recovery",
                            "instructions": "Stand on one leg, grab your other foot and pull it toward your glute. Keep your knees together.",
                            "cues": [
                                "Stand tall",
                                "Hips forward"
                            ],
                            "durationSeconds": 240
                        }
                    ]
                }
            ]
        },
        {
            "id": "cali-w1-d5",
            "name": "Fat Burn (Low Impact)",
            "blocks": [
                {
                    "id": "warmup",
                    "name": "Warm-up",
                    "type": "warmup",
                    "rounds": 1,
                    "steps": [
                        {
                            "id": "w1",
                            "type": "timer",
                            "name": "Torso Twists",
                            "description": "Core warmup",
                            "instructions": "Stand with feet hip-width and rotate your torso left and right, letting your arms swing freely.",
                            "cues": [
                                "Loose arms",
                                "Pivot feet slightly"
                            ],
                            "durationSeconds": 240
                        }
                    ]
                },
                {
                    "id": "circuit",
                    "name": "Main Circuit",
                    "type": "circuit",
                    "rounds": 4,
                    "steps": [
                        {
                            "id": "c1",
                            "type": "exercise_inline",
                            "name": "Marching in place",
                            "description": "Steady cardio",
                            "instructions": "Drive your knees high and swing your arms. Maintain a steady pace that gets your heart rate up.",
                            "cues": [
                                "High knees",
                                "Active arms"
                            ],
                            "durationSeconds": 30
                        },
                        {
                            "id": "c2",
                            "type": "exercise_inline",
                            "name": "Shadow boxing",
                            "description": "Upper body cardio",
                            "instructions": "Perform light punches into the air. Keep your feet moving and rotate your core with each punch.",
                            "cues": [
                                "Relaxed shoulders",
                                "Keep moving"
                            ],
                            "durationSeconds": 30
                        },
                        {
                            "id": "c3",
                            "type": "exercise_inline",
                            "name": "Standing oblique crunch",
                            "description": "Core and balance",
                            "instructions": "Bring your knee up to the side while simultaneously bringing your elbow down to meet it. Squeeze your side.",
                            "cues": [
                                "Squeeze obliques",
                                "Exhale on crunch"
                            ],
                            "durationSeconds": 30
                        }
                    ]
                },
                {
                    "id": "cooldown",
                    "name": "Cooldown",
                    "type": "cooldown",
                    "rounds": 1,
                    "steps": [
                        {
                            "id": "cd1",
                            "type": "hold",
                            "name": "Full Body Relaxation",
                            "description": "Calm recovery",
                            "instructions": "Stand still, close your eyes, and take several deep breaths. Let all tension drain away.",
                            "cues": [
                                "Breathe out slow",
                                "Relax face and jaw"
                            ],
                            "durationSeconds": 240
                        }
                    ]
                }
            ]
        }
    ]
}