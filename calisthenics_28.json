            "name": "Upper Body Focus",
            "blocks": [
                {
                    "id": "warmup",
                    "name": "Warm-up",
                    "type": "warmup",
                    "rounds": 1,
                    "steps": [
                        {
                            "id": "w1",
                            "type": "timer",
                            "name": "Arm Circles",
                            "description": "Shoulder mobility",
                            "instructions": "Extend arms to the sides and perform small, then large circles. Reverse direction halfway through.",
                            "cues": [
                                "Keep arms straight",
                                "Wide circles"
                            ],
                            "durationSeconds": 240
                        }
                    ]
                },
                {
                    "id": "circuit",
                    "name": "Main Circuit",
                    "type": "circuit",
                    "rounds": 3,
                    "steps": [
                        {
                            "id": "c1",
                            "type": "exercise_inline",
                            "name": "Push-ups",
                            "description": "Foundational strength",
                            "instructions": "Place your hands slightly wider than shoulder-width. Lower your chest until it nearly touches the floor, then push back up. Keep your body straight.",
                            "cues": [
                                "No sagging back",
                                "Full range of motion"
                            ],
                            "reps": 10
                        },
                        {
                            "id": "c2",
                            "type": "exercise_inline",
                            "name": "Pike Hold",
                            "description": "Shoulder stability",
                            "instructions": "From a pushup position, walk your feet in and lift your hips high, forming an inverted V. Hold your weight on your shoulders.",
                            "cues": [
                                "Hips high",
                                "Look at your feet"
                            ],
                            "durationSeconds": 30
                        },
                        {
                            "id": "c3",
                            "type": "exercise_inline",
                            "name": "Triceps Dips",
                            "description": "Arm definition",
                            "instructions": "Sit on the edge of a chair or bench, hands next to hips. Slide off and lower your body by bending your elbows, then push back up.",
                            "cues": [
                                "Elbows back",
                                "Chest up"
                            ],
                            "reps": 12
                        }
                    ]
                },
                {
                    "id": "cooldown",
                    "name": "Cooldown",
                    "type": "cooldown",
                    "rounds": 1,
                    "steps": [
                        {
                            "id": "cd1",
                            "type": "hold",
                            "name": "Arm Stretch",
                            "description": "Triceps and chest",
                            "instructions": "Stretch each arm across your body and also reach overhead to touch the opposite shoulder blade.",
                            "cues": [
                                "Gentle pulling",
                                "Breathe into it"
                            ],
                            "durationSeconds": 240
                        }
                    ]
                }
            ]
        },
        {
            "id": "cali-w1-d4",
            "name": "Low-Impact Legs",
            "blocks": [
                {
                    "id": "warmup",
                    "name": "Warm-up",
                    "type": "warmup",
                    "rounds": 1,
                    "steps": [
                        {
                            "id": "w1",
                            "type": "timer",
                            "name": "Leg Swings",
                            "description": "Hip mobility",
                            "instructions": "Hold onto a wall and swing one leg forward and backward, then side-to-side. Switch legs halfway.",
                            "cues": [
                                "Keep torso still",
                                "Broad swings"
                            ],
                            "durationSeconds": 240
                        }
                    ]
                },
